# FFTT API Smartping 2.0 Credentials
# Copy this file to .env and fill in your actual credentials

# Application ID provided by FFTT (required)
FFTT_APP_ID=your_app_id_here

# Password for API authentication (required)
FFTT_PASSWORD=your_password_here

# Optional series identifier (15 alphanumeric characters)
# If not provided, a random one will be generated
FFTT_SERIE=your_serie_here
